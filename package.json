{
    "name": "toei_signage_frontend",
    "version": "2.3.0",
    "private": true,
    "scripts": {
        "start": "node dist/server/index",
        "ssl": "node dist/server/ssl",
        "serve": "vue-cli-service serve",
        "build-client": "vue-cli-service build",
        "build-client-local": "vue-cli-service build --watch",
        "build-server": "tsc --project src/server --outDir dist/server --locale ja-jp",
        "build": "npm run build-server && npm run build-client",
        "lint": "vue-cli-service lint",
        "test": "npm run lint",
        "replaceBuildTimestamp": "replace-in-file TIMESTAMPINJECTEDINBUILDPROCESS %date%-%time% dist/client/env/index.php && replace-in-file TIMESTAMPINJECTEDINBUILDPROCESS %date%-%time% dist/server/app/routes/api/util.js",
        "version": "git add -A"
    },
    "dependencies": {
        "axios": "^0.19.0",
        "basic-auth": "^2.0.1",
        "body-parser": "^1.19.0",
        "compression": "^1.7.4",
        "core-js": "^3.6.1",
        "dayjs": "^1.8.18",
        "debug": "^4.3.1",
        "deep-diff": "^1.0.2",
        "es6-promise": "^4.2.8",
        "express": "^4.17.1",
        "helmet": "^4.4.1",
        "http-status": "^1.5.0",
        "moment": "^2.29.1",
        "normalize.css": "^8.0.1",
        "vue": "^2.6.11",
        "vue-router": "^3.1.3",
        "vuex": "^3.1.2",
        "zen-observable": "^0.8.15"
    },
    "devDependencies": {
        "@types/basic-auth": "^1.1.3",
        "@types/compression": "^1.7.0",
        "@types/debug": "^4.1.5",
        "@types/express": "^4.17.11",
        "@types/helmet": "^4.0.0",
        "@types/request-promise-native": "^1.0.17",
        "@types/zen-observable": "^0.8.0",
        "@typescript-eslint/eslint-plugin": "^2.14.0",
        "@typescript-eslint/parser": "^2.14.0",
        "@vue/cli-plugin-babel": "^4.1.2",
        "@vue/cli-plugin-eslint": "^4.1.2",
        "@vue/cli-plugin-router": "^4.1.2",
        "@vue/cli-plugin-typescript": "^4.1.2",
        "@vue/cli-plugin-vuex": "^4.1.2",
        "@vue/cli-service": "^4.1.2",
        "@vue/eslint-config-prettier": "^6.0.0",
        "@vue/eslint-config-typescript": "^5.0.1",
        "eslint": "^6.8.0",
        "eslint-plugin-prettier": "^3.1.2",
        "eslint-plugin-vue": "^6.1.2",
        "prettier": "^1.18.2",
        "replace-in-file": "^5.0.2",
        "sass": "^1.24.2",
        "sass-loader": "^8.0.0",
        "typescript": "3.7.4",
        "vue-cli-plugin-webpack-bundle-analyzer": "^2.0.0",
        "vue-template-compiler": "^2.6.11"
    },
    "gitHooks": {
        "pre-commit": "npm run build && npm run replaceBuildTimestamp && git add --all"
    }
}
